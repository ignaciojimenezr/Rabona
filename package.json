{
  "name": "rabona",
  "version": "1.0.0",
  "description": "Rabona - A soccer-themed tic-tac-toe game built as an OpenAI App with Cloudflare Workers",
  "main": "src/worker.ts",
  "type": "module",
  "scripts": {
    "build": "npm run bundle-data && tsc",
    "bundle-data": "node scripts/bundle-data.js",
    "dev": "npm run bundle-data && wrangler dev",
    "deploy": "npm run bundle-data && wrangler deploy",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "openai",
    "openai-apps",
    "tic-tac-toe",
    "soccer",
    "football",
    "cloudflare-workers",
    "mcp",
    "model-context-protocol",
    "game"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.23.0",
    "csv-parse": "^5.5.5",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241106.0",
    "@types/node": "^20.10.0",
    "typescript": "^5.3.3",
    "wrangler": "^4.53.0"
  }
}
